<p id="notice"><%= notice %></p>

<h1>Vinos</h1>
<%= link_to "enologo", enologos_path %>
<%= link_to "vinos", vinos_path %>
<%= link_to "Notas y enologo", enologo_vinos_path %>
 <%= link_to "sing out", destroy_user_session_path, method: :delete, class:"btn btn-outline-primary"%>
<table>
  <thead>
    <tr>
      <th>Cepas</th>
      <th>Porcentaje</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vinos.each do |vino| %>
      <tr>
        <td><%= vino.cepas %></td>
        <td><%= vino.porcentaje %></td>
        <td><%= link_to 'Show', vino %></td>
        <td><%= link_to 'Edit', edit_vino_path(vino) %></td>
        <td><%= link_to 'Destroy', vino, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

